{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CookieSettings": {
    "Audience": "VoroFit.API_Audience",
    "Issuer": "VoroFit.API_Issuer",
    "Name": "VoroFit",
    "AccessDeniedPath": "/Unauthorized",
    "LoginPath": "/SignIn",
    "Path": "/",
    "ExpireHours": 2
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "https://fit.vorolabs.app"
    ]
  }
}
